

"UNV_BRANCH_DB": "Univ_ProgramWiseBranches",UnvBranchDbName, UNV_GET_Branches(insert)

"UNV_ACCR_DB": "Univ_Accreditations",  UnvAccrDbName,UNV_GET_Accredations(get)
"UNV_PROG_DB": "Univ_ProgramsOffered", UNV_GET_Program (get)

"UNV_MASTER_DB_NAME": "University_Master",
"UNV_SPLOFR_DB": "Univ_SplOffers",

SELECT id,TieupName,TieupType,TieupDescription,TiedupWith_Name,TieUp_PhoneNumber,TieUp-EmailID,Stakeholder_ID,AttachFile,AttachFile_Name,Start_Date,End_Date,EnabledFlag,PublishFlag,CreationDate,LastUpdatedDate FROM CollabToHire.Univ_Tieups WHERE Stakeholder_ID=? GROUP BY id,TieupName,TieupType,TieupDescription,TiedupWith_Name,TieUp_PhoneNumber,TieUp-EmailID,Stakeholder_ID,AttachFile,PublishFlag,AttachFile_Name,Start_Date,End_Date,EnabledFlag,CreationDate,LastUpdatedDate ORDER BY CreationDate
